{
    "builds": [{
        "src": "resume_api/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "iii.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "static" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "resume_api/wsgi.py"
        }
    ]
  }
  