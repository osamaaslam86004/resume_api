{
    "builds": [{
        "src": "resume_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    },
    {
        "src": "iii.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "static" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "resume_api/wsgi.py"
        }
    ]
}
    
  